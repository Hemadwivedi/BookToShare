<div class="availablecontent">
<div class="imagebrowse">
        <img src="/assets/images/browse.png" style="width:350px; height: 300px;" class="girlreading">
    </div>
<div id="search">
    <form id="myform">
        <div class="input-field">
            
        </div>
        <button class="btn btn-search1" type="button" id="search-btn"> Add Another Book</button>
    </form>
  </div>
  <div class="allbooksdisplayed">
{{#each books}}

    <div class="card-deck">
        <div class="card">
            <img class="card-img-top" src="{{jpegImg}}" alt="">

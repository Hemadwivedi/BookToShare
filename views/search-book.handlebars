<div class="availablecontent">
<div class="imagebrowse">
        <img src="/assets/images/browse.png" style="width:350px; height: 300px;" class="girlreading">
    </div>
<div class="input-group mb-3">
    <input class="form-control" type="text" placeholder="Find By Name..." id="search-box">
    <div class="input-group-append">
        <button class="btn btn-outline-secondary" type="button">Search</button>
    </div>
</div>
{{#each books}}
        <div class="card">
            <img class="card-img-top" src="{{jpegImg}}" alt="">
            <div class="card-body">
                <h4 class="card-title">{{title}}</h4>
                <p class="card-text">{{description}}</p>
                <a class="btn btn-success btn-block" href="#">buy</a>
            </div>
        </div>
{{/each}}
<h5 class="linktoapi">Didn't find the book you were looking for? We got you! <a href="/search-apibook">Click me!</a></h5>
</div>
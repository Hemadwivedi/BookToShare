<div id="search">
    <form id="myform">
        <div class="input-field">
            <input type="search" id="search-box" class="form-control" placeholder="Find By Title" aria-describedby="button-addon2">
        </div>
        <button class="btn btn-search1" type="button" id="search-btn">Search</button>
    </form>
  </div>
  <div class="allbooksdisplayed">
{{#each books}}

    <div class="card-deck">
        <div class="card">
            <img class="card-img-top" src="{{jpegImg}}" alt="">
            <div class="card-body">
                <h4 class="card-title">{{title}}</h4>
                <p class="card-text">{{description}}</p>
                <a class="btn btn-block" href="#">Buy</a>
            </div>
        </div>   
    </div> 

{{/each}}

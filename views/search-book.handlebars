                <h4 class="card-title">{{title}}</h4>
                <p class="card-text">{{description}}</p>
                <a class="btn btn-block" href="#">Buy</a>
                <a class="btn btn-block" href="#">Edit</a>
            </div>
        </div>   
    </div> 

{{/each}}
